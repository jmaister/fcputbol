{
    "name": "default",
    "type": "sqlite",
    "database": "fcputbol.sqlite",
    "synchronize": true,
    "logging": true,
    "entities": [
        "db/entities/**/*.entity.{ts,js}"
    ],
    "migrations": [
        "db/migration/**/*.ts"
    ],
    "subscribers": [
        "db/subscriber/**/*.ts"
    ],
    "cli": {
        "entitiesDir": "db/entities",
        "migrationsDir": "db/migrations"
    },
    "migrationsTableName": "migrations_table"
}
